source "https://rubygems.org"

# Jekyll and theme dependencies
gem "jekyll"
gem "minimal-mistakes-jekyll"

# Required plugins for Minimal Mistakes theme
group :jekyll_plugins do
gem "jekyll-paginate"
gem "jekyll-sitemap"
gem "jekyll-gist"
gem "jekyll-feed"
gem "jekyll-include-cache"
end

# Required for Ruby 3.x compatibility
gem "webrick"
